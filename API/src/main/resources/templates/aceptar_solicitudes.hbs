<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>Miembro</th>
            <th>Sector</th>
            <th>Activa</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    {{#each solicitudes}}
        <tr>
            <td>{{id}}</td>
            <td>{{miembro.text}}</td>
            <td>{{sector.text}}</td>
            <td>SI</td>
            <td>
                <button type="button" class="btn btn-success" onclick="onAccept({{id}})">Aceptar</button>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>

